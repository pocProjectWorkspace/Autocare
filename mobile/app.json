{
    "expo": {
        "name": "AutoCare",
        "slug": "autocare",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "automatic",
        "scheme": "autocare",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#0A0E27"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.autocare.app"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#0A0E27"
            },
            "package": "com.autocare.app"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            "expo-router",
            "expo-secure-store",
            "expo-image-picker",
            "expo-document-picker",
            "expo-location"
        ],
        "experiments": {
            "typedRoutes": true
        }
    }
}